{
    "name": "smpleader/spt-examples",
    "type": "project",
    "description": "The examples showoff SPT Framework.",
    "keywords": [
        "framework",
        "spt"
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^7.0",
        "smpleader/spt": "^0.4.0",
        "joomla/di": "~1.3",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/events": "^8.0"
    },
    "require-dev": { 
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Examples\\": "examples/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
         
    }
}
